{
  "name": "homepage",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://u-ten.github.io/homepage",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:prod": "NODE_ENV=production next build",
    "predeploy": "rm -rf .next .next-prod out && npm run build:prod",
    "deploy": "git add out/ -f && git commit -m \"Deploy to gh-pages with .nojekyll\" && git push origin `git subtree split --prefix out main`:gh-pages --force"
  },
  "dependencies": {
    "next": "15.2.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "swiper": "^11.2.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.17",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "gh-pages": "^6.1.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  }
}
